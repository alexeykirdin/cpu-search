{
  "manifest_version": 2,
  "name": "CPUBench",
  "description": "Search CPUBenchmark on the go!",
  "version": "1.0",
  "icons": {
    "128": "images/ico-120x120.png",
    "48": "images/ico-60x60.png",
    "16": "images/ico-16x16.png"
  },
  "background": {
    "scripts": [
      "event-page.js"
    ],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
  ]
}
